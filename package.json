    "@typescript-eslint/parser": "^8.0.0",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.24.0",
    "obsidian": "latest",
    "tslib": "^2.8.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "googleapis": "^144.0.0",
    "monkey-around": "^3.0.0"
